{
    "name": "alien/yii2-jquery-formbuilder",
    "description": "Widget for jQuery fromBuilder",
    "type": "yii2-extension",
    "keywords": ["yii2","formBuilder"],
	"repositories": [
    {
        "type": "composer",
        "url": "https://asset-packagist.org"
    }
]
    "authors": [
        {
        "name": "Alien-art",
        "email": "alien@alien-art.ru"
        }
    ],
	"minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "*",
		"yiisoft/yii2-jui": "^2.0.0",
        "bower-asset/formBuilder": "@stable"
    },
    "autoload": {
        "psr-4": {
            "alien\\jquery_formbuilder\\": ""
        }
    },
	"extra": {
    "asset-installer-paths": {
	  "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
	}
}